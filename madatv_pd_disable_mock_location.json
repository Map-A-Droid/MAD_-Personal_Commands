{
  "ATV - PD mock location patch: disable": [
    {
      "TYPE": "jobType.PASSTHROUGH",
      "SYNTAX": "pdconf=\"/data/data/com.mad.pogodroid/shared_prefs/com.mad.pogodroid_preferences.xml\" && sed -i 's,\"switch_enable_mock_location_patch\" value=\"true\",\"switch_enable_mock_location_patch\" value=\"false\",g' \"$pdconf\" && chmod 660 \"$pdconf\" && puser=$(ls -la /data/data/com.mad.pogodroid/|head -n2|tail -n1|awk '{print $3}') && chown \"$puser\":\"$puser\" \"$pdconf\""
    },
    {
      "TYPE": "jobType.REBOOT",
      "SYNTAX": "su -c reboot"
    }
  ]
}
